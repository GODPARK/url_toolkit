{
    "name": "UrlToolkit",
    "description": "URL의 가시성을 높이고 URL을 보다 손쉽게 제어하여 페이지 이동을 할 수 있는 툴입니다.",
    "version": "0.1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "48": "main_icon48.png",
        "128": "main_icon128.png"
}
}
